{
    "name": "remote-vm",
    "displayName": "Remote VM",
    "description": "Automatically create, setup and connect to Digital Ocean VM for effective remote development",
    "version": "0.2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/iskorotkov/remote-vm.git"
    },
    "author": {
        "name": "Ivan Korotkov",
        "email": "korotkov.ivan.s@gmail.com",
        "url": "https://github.com/iskorotkov"
    },
    "publisher": "iskorotkov",
    "icon": "resources/icon.png",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:remote-vm.login",
        "onUri"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "remote-vm.login",
                "title": "Remote VM: Login"
            }
        ]
    },
    "scripts": {
        "publish": "vsce package",
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.53.0",
        "@typescript-eslint/eslint-plugin": "^4.16.1",
        "@typescript-eslint/parser": "^4.16.1",
        "esbuild": "^0.11.16",
        "eslint": "^7.12.1",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "typescript": "^4.1.3",
        "vsce": "^1.99.0",
        "vscode-test": "^1.5.0"
    },
    "dependencies": {
        "client-oauth2": "^4.3.3",
        "query-string": "^7.0.1",
        "tslib": "^2.1.0"
    }
}
